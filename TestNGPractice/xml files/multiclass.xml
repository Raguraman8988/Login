<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="classes">
	<test thread-count="5" name="Test" parallel="classes">
		<groups>
			<run>
				<include name="150cc"></include>
				<exclude name="100cc"></exclude>
				<include name="men"></include>
				<exclude name="kids"></exclude>
			</run>
		</groups>
		<classes>
			<class name="com.testngtraining.org.amazonSearch" />
			<class name="com.testngtraining.org.bikeSearch" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
